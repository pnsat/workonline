<?xml version='1.0' encoding='utf-8' ?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/bg_blue"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include
            layout="@layout/header_menu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@id/setmachine_text_seting"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10.0dip"
            android:text="ตั้งค่าตู้เติมเงิน"
            android:textAppearance="?android:textAppearanceSmall"
            android:textColor="@color/black"
            android:textSize="27.0sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10.0dip">

            <TextView
                android:id="@id/setmachine_text_partner_id"
                android:layout_width="230.0dip"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="10.0dip"
                android:text="PARTNER ID"
                android:textAppearance="?android:textAppearanceSmall"
                android:textColor="@color/black"
                android:textSize="17.0sp"
                android:textStyle="bold" />

            <EditText
                android:id="@id/EditText05"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="10.0dip"
                android:layout_weight="1.0"
                android:ems="10"
                android:focusable="false"
                android:singleLine="true" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10.0dip">

            <TextView
                android:id="@id/setmachine_text_hardware_sn"
                android:layout_width="230.0dip"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="10.0dip"
                android:text="HARDWARE SN"
                android:textAppearance="?android:textAppearanceSmall"
                android:textColor="@color/black"
                android:textSize="17.0sp"
                android:textStyle="bold" />

            <EditText
                android:id="@id/month"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="10.0dip"
                android:layout_weight="1.0"
                android:ems="10"
                android:focusable="false"
                android:singleLine="true" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10.0dip">

            <TextView
                android:id="@id/setmachine_text_hardware_au"
                android:layout_width="230.0dip"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="10.0dip"
                android:text="HARDWARE AUTHORIZATION"
                android:textAppearance="?android:textAppearanceSmall"
                android:textColor="@color/black"
                android:textSize="17.0sp"
                android:textStyle="bold" />
        </LinearLayout>

        <EditText
            android:id="@id/day"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="10.0dip"
            android:layout_weight="1.0"
            android:ems="10"
            android:focusable="false"
            android:singleLine="true">

            <requestFocus />
        </EditText>

        <CheckBox
            android:id="@id/setting_AUTORESET"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Auto Reset Money"
            android:textColor="@color/black" />

        <CheckBox
            android:id="@id/setting_ict"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="top"
            android:text="Bill Acceptor ICT"
            android:textColor="@color/black" />

        <CheckBox
            android:id="@id/setting_debug"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TestMode"
            android:textColor="@color/black" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:weightSum="3.0">

            <Button
                android:id="@id/SetMechine_SMS"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10.0dip"
                android:layout_weight="1.0"
                android:text="SMS"
                android:textSize="18.0sp"
                android:textStyle="bold" />

            <Button
                android:id="@id/ExitSetting"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10.0dip"
                android:layout_weight="1.0"
                android:text="Setting"
                android:textSize="18.0sp"
                android:textStyle="bold" />

            <Button
                android:id="@id/Button12"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10.0dip"
                android:layout_weight="1.0"
                android:text="Update"
                android:textColor="@color/brown"
                android:textSize="18.0sp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:weightSum="3.0">

            <Button
                android:id="@id/SetupUSSD"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10.0dip"
                android:layout_weight="1.0"
                android:text="setussd"
                android:textSize="18.0sp"
                android:textStyle="bold" />

            <Button
                android:id="@id/testcallUSSD"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10.0dip"
                android:layout_weight="1.0"
                android:text="test ussd Money"
                android:textSize="18.0sp"
                android:textStyle="bold" />

            <Button
                android:id="@id/testcallUSSD2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10.0dip"
                android:layout_weight="1.0"
                android:text="test ussd Phone"
                android:textSize="18.0sp"
                android:textStyle="bold" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center"
        android:layout_marginTop="10.0dip">

        <Button
            android:id="@id/fake_click"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bt_test_network_th"
            android:textStyle="bold"
            android:visibility="gone" />

        <Button
            android:id="@id/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bt_record_th" />

        <Button
            android:id="@id/button3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bt_back_th" />
    </LinearLayout>

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center"
        android:visibility="gone">

        <include
            layout="@layout/footer_menu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </FrameLayout>
</FrameLayout>
